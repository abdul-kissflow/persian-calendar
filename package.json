{
    "name": "persian-calendar",
    "version": "1.0.0",
    "description": "Custom form field for Kissflow's form",
    "main": "index.js",
    "scripts": {
        "dev": "form-field-scripts dev",
        "build": "form-field-scripts build",
        "zip": "form-field-scripts zip"
    },
    "type": "module",
    "author": "",
    "license": "ISC",
    "devDependencies": {
        "@kissflow/form-field-config": "0.0.2",
        "@kissflow/form-field-scripts": "0.0.7"
    },
    "engines": {
        "node": ">=16.0.0 <17.0.0"
    },
    "eslintConfig": {
        "extends": [
            "react-app"
        ],
        "rules": {
            "no-restricted-globals": [
                "error",
                {
                    "name": "window",
                    "message": "Don't use browser's `window` object."
                },
                {
                    "name": "document",
                    "message": "Don't use browser's `document` object."
                },
                {
                    "name": "fetch",
                    "message": "Using browser's fetch api is not allowed."
                },
                {
                    "name": "XMLHttpRequest",
                    "message": "Using browser's XMLHttpRequest is not allowed."
                }
            ]
        }
    },
    "keywords": [
        "lcnc",
        "no-code",
        "low-code",
        "platform",
        "react",
        "cli",
        "kissflow",
        "custom-components",
        "custom-fields"
    ],
    "dependencies": {
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-multi-date-picker": "^4.5.2"
    }
}
